<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.br.atividade.calculos">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name=".CalculoImc"
            android:label="@string/title_activity_calculo_imc"
            android:theme="@style/AppTheme.NoActionBar" />
        <activity
            android:name=".CalculoFinanciamento"
            android:label="@string/title_activity_calculo_financiamento"
            android:theme="@style/AppTheme.NoActionBar" />
        <activity android:name=".SegundaTela" />
        <activity android:name=".ListViewTimes"></activity>
    </application>

</manifest>